programa {
    cadeia nome
    inteiro anonascimento
    inteiro idade
    inteiro anoatual
    inteiro diferenca
    funcao inicio() {
    escreva ("qual o seu nome?")
    leia (nome)
    escreva ("qual o seu ano de nascimento?")
    leia (anonascimento)
    idade = (2024-anonascimento)
    diferenca = (50-idade)
    escreva("ola"+ nome +"voce tem"+ idade +"anos, e tera 50 anos em"+ diferenca +"anos")
  }
}
